    "components": {
        "schemas": {
            "LoginRequest": {
                "title": "LoginRequest",
                "description": "Validation rules for user login",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "User's email",
                        "type": "string",
                        "format": "email",
                        "example": "user@example.com"
                    },
                    "password": {
                        "description": "User's password",
                        "type": "string",
                        "format": "password",
                        "example": "password"
                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "title": "RegisterRequest",
                "description": "Validation rules for user registration",
                "required": [
                    "name",
                    "last_name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "description": "User's name",
                        "type": "string",
                        "example": "John"
                    },
                    "last_name": {
                        "description": "User's last name",
                        "type": "string",
                        "example": "Doe"
                    },
                    "email": {
                        "description": "User's email",
                        "type": "string",
                        "format": "email",
                        "example": "john.doe@example.com"
                    },
                    "password": {
                        "description": "User's password",
                        "type": "string",
                        "format": "password",
                        "example": "Password@123"
                    },
                    "password_confirmation": {
                        "description": "Confirmation of user's password",
                        "type": "string",
                        "format": "password",
                        "example": "Password@123"
                    }
                },
                "type": "object"
            },
            "CourseCreateRequest": {
                "title": "CourseCreateRequest",
                "description": "Course creation request",
                "required": [
                    "name",
                    "start_date",
                    "end_date",
                    "course_type_id"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Course Name"
                    },
                    "start_date": {
                        "type": "string",
                        "format": "date",
                        "example": "2024-04-25"
                    },
                    "end_date": {
                        "type": "string",
                        "format": "date",
                        "example": "2024-05-25"
                    },
                    "course_type_id": {
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "CourseUpdateRequest": {
                "title": "CourseUpdateRequest",
                "description": "Course update request",
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Updated Course Name"
                    },
                    "start_date": {
                        "type": "string",
                        "format": "date",
                        "example": "2024-04-25"
                    },
                    "end_date": {
                        "type": "string",
                        "format": "date",
                        "example": "2024-05-25"
                    },
                    "course_type_id": {
                        "type": "integer",
                        "example": 1
                    }
                },
                "type": "object"
            },
            "UnsubscribeUserCourseRequest": {
                "title": "Unsubscribe User from Course Request",
                "required": [
                    "course_id",
                    "student_id"
                ],
                "properties": {
                    "course_id": {
                        "description": "ID of the course",
                        "type": "integer",
                        "example": "1"
                    },
                    "student_id": {
                        "description": "ID of the student",
                        "type": "integer",
                        "example": "1"
                    }
                },
                "type": "object"
            },
            "Course": {
                "title": "Course",
                "description": "Course model",
                "required": [
                    "name",
                    "start_date",
                    "end_date",
                    "course_type_id"
                ],
                "properties": {
                    "id": {
                        "description": "Course ID",
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "description": "Course name",
                        "type": "string",
                        "example": "Mathematics"
                    },
                    "start_date": {
                        "description": "Course start date",
                        "type": "string",
                        "format": "date",
                        "example": "2024-05-01"
                    },
                    "end_date": {
                        "description": "Course end date",
                        "type": "string",
                        "format": "date",
                        "example": "2024-07-01"
                    },
                    "course_type_id": {
                        "description": "ID of the course type",
                        "type": "integer",
                        "example": 1
                    },
                    "type": {
                        "$ref": "#/components/schemas/CourseType"
                    }
                },
                "type": "object"
            },
            "CourseType": {
                "title": "CourseType",
                "description": "CourseType model",
                "required": [
                    "name"
                ],
                "properties": {
                    "id": {
